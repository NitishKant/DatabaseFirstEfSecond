@{
       var example = ViewData.Model;   
}
@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>
<table class="table">
    <thead>
        <tr>
            <th>Name</th>  
            <th>Class</th>  
            <th>Course Name</th>
        </tr>
    </thead>
    <tbody>
       @foreach(var item in example)
        {
         <tr> 
            <td>@item.Name</td>
            <td>@item.Class</td>
            <td>@item.CourseName</td>
         </tr> 
        }
    </tbody>
</table> 
